     <property name="styleSheet">
      <string notr="true">/* 纤细现代风格滚动条 */
QScrollArea {
    border: none;
    background: transparent;
}

QScrollArea &gt; QWidget {
    background-color: #f8f9fa; /* 内容区域背景色 */
}

/* 滚动条整体（更细） */
QScrollBar:vertical {
    width: 8px; /* 垂直滚动条宽度 */
    background: transparent; /* 背景透明 */
    margin: 0;
}

QScrollBar:horizontal {
    height: 8px; /* 水平滚动条高度 */
    background: transparent;
    margin: 0;
}

/* 滑块（更精致） */
QScrollBar::handle:vertical, QScrollBar::handle:horizontal {
    background: rgba(150, 150, 150, 0.4); /* 半透明灰色 */
    border-radius: 4px; /* 稍小的圆角 */
    min-width: 8px; /* 最小宽度（防止过细） */
    min-height: 8px; /* 最小高度（防止过细） */
}

QScrollBar::handle:vertical:hover, 
QScrollBar::handle:horizontal:hover {
    background: rgba(120, 120, 120, 0.6); /* 悬停时更深 */
}

QScrollBar::handle:vertical:pressed, 
QScrollBar::handle:horizontal:pressed {
    background: rgba(100, 100, 100, 0.8); /* 点击时更深 */
}

/* 隐藏滚动条按钮和角落区域 */
QScrollBar::add-line, QScrollBar::sub-line,
QScrollBar::add-page, QScrollBar::sub-page {
    background: none;
    height: 0px;
    width: 0px;
}</string>
