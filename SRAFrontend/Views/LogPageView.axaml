<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:suki="https://github.com/kikipoulet/SukiUI"
             xmlns:lang="clr-namespace:SRAFrontend.Localization"
             xmlns:viewModels="clr-namespace:SRAFrontend.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="SRAFrontend.Views.LogPageView"
             x:DataType="viewModels:LogPageViewModel">
    <Design.DataContext>
        <viewModels:LogPageViewModel />
    </Design.DataContext>
    <suki:GlassCard>
        <suki:GroupBox Header="{x:Static lang:Resources.LogText}">
            <TextBlock Text="{Binding Log}" />
        </suki:GroupBox>
    </suki:GlassCard>
</UserControl>
